{
  "name": "discord-lookup-api",
  "author": "Lucas",
  "version": "3.0.0",
  "description": "A simple API to lookup for a Discord User or Bot ID.",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "predev": "yarn swagger",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec\"",
    "build": "tsc",
    "prebuild": "yarn swagger",
    "test": "jest",
    "swagger": "tsoa spec"
  },
  "license": "ISC",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.6.2",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "pg": "^8.7.1",
    "sequelize": "^6.12.5",
    "swagger-ui-express": "^4.5.0",
    "ts-node": "^10.4.0",
    "tsoa": "^4.1.3",
    "typescript": "^4.5.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/node": "^16.11.12",
    "@types/swagger-ui-express": "^4.1.3",
    "axios": "^0.24.0",
    "concurrently": "^7.4.0",
    "jest": "^28.1.3",
    "nodemon": "^2.0.15",
    "ts-jest": "^28.0.7",
    "typescript": "^4.5.3",
    "xhr-mock": "^2.5.1"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  }
}
