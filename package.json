{
  "name": "discord-lookup-api",
  "author": "Lucas",
  "version": "3.0.0",
  "description": "A simple API to lookup for a Discord User or Bot ID.",
  "main": "index.ts",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "swc src -d dist --source-maps --copy-files",
